{
  "name": "ankorar-api",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm services:up && pnpm services:wait:database && pnpm migrate:latest && pnpm start:dev",
    "build": "tsc --noEmit",
    "lint": "pnpm lint:prettier:check && pnpm lint:eslint:check .",
    "start:dev": "tsx watch --include \"./src/**/*.ts\" --exclude \"./src/**/*.spec.ts\" --env-file=./.env ./src/index.ts",
    "test:e2e:watch": "pnpm services:up && pnpm services:wait:database && vitest run --config ./vitest.config.e2e.ts --watch",
    "test:e2e:ui": "pnpm services:up && pnpm services:wait:database && vitest run --config ./vitest.config.e2e.ts --ui",
    "test:e2e:watch:only": "vitest run --config ./vitest.config.e2e.ts --watch",
    "test:e2e": "pnpm services:up && pnpm services:wait:database && vitest run --config ./vitest.config.e2e.ts",
    "posttest": "pnpm services:down",
    "services:up": "docker compose -f ./infra/docker-compose.dev.yaml -p ankorar up -d",
    "services:down": "docker compose -p ankorar down",
    "services:stop": "docker compose -p ankorar stop",
    "services:wait:database": "node ./infra/scripts/wait-for-postgres.js",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:eslint:check": "eslint",
    "lint:eslint:fix": "eslint --fix",
    "migrate:latest": "prisma migrate dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.27.0",
  "dependencies": {
    "@fastify/cors": "11.2.0",
    "@fastify/swagger": "9.6.1",
    "@prisma/adapter-pg": "7.3.0",
    "@prisma/client": "7.3.0",
    "@scalar/fastify-api-reference": "1.43.1",
    "async-retry": "1.3.3",
    "bcryptjs": "3.0.3",
    "cookie": "1.1.1",
    "dayjs": "1.11.13",
    "drizzle-orm": "0.45.1",
    "fastify": "5.6.2",
    "fastify-type-provider-zod": "6.1.0",
    "jsonwebtoken": "9.0.2",
    "nodemailer": "7.0.12",
    "pg": "8.16.3",
    "stripe": "20.3.1",
    "uuid": "13.0.0",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@faker-js/faker": "10.2.0",
    "@types/async-retry": "1.4.9",
    "@types/bcryptjs": "3.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "25.0.3",
    "@types/nodemailer": "7.0.5",
    "@types/pg": "8.16.0",
    "@types/set-cookie-parser": "2.4.10",
    "@vitest/ui": "4.0.16",
    "concurrently": "9.2.1",
    "detect-port": "2.1.0",
    "dotenv": "17.2.3",
    "drizzle-kit": "0.31.8",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "globals": "17.0.0",
    "jiti": "2.6.1",
    "prettier": "3.7.4",
    "prisma": "7.3.0",
    "set-cookie-parser": "3.0.1",
    "tsup": "8.5.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.52.0",
    "unplugin-swc": "1.5.9",
    "vite-tsconfig-paths": "6.0.3",
    "vitest": "4.0.16"
  }
}
